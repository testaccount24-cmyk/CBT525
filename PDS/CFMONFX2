//*  CORRECTS ABEND S0C9 AT CFXCFCON+06AC IN CFMON ADDRESS SPACE
//*
//APPLY   EXEC PGM=AMASPZAP,PARM=IGNIDRFULL
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=SYSx.CFMON.LOAD
//SYSIN    DD  *
NAME CFXCFCON CFXCFCON
VER 000000 47F0,F04C                B START
REP 000000 47F0,F048                B START-4
VER 000048 61F9,F800
REP 000048 1F33,0430                SLR R3,R3 SPM R3



